{
    "name": "mydansun/laravel-math",
    "description": "High precision calculation library for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Sun",
            "email": "mydansun@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "krowinski/bcmath-extended": "^4.2",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x"
    },
    "autoload": {
        "psr-4": {
            "Mydansun\\Math\\": "src/Math/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mydansun\\Math\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "Math": "Mydansun\\Math\\Facades\\Math"
            }
        }
    }
}
